target_host=arm-none-eabi
cc_compiler=gcc
cxx_compiler=g++

[env]
CHOST=$target_host
AR=$target_host-ar
AS=$target_host-as
RANLIB=$target_host-ranlib
CC=$target_host-$cc_compiler
CXX=$target_host-$cxx_compiler
STRIP=$target_host-strip

[settings]
arch=armv7
compiler=gcc
compiler.version=11
compiler.libcxx=libstdc++11
build_type=MinSizeRel
